var j = '<%= j render(:partial => 'arguments/show', :locals => {:path_method => :question_argument_path, :node=> @node, :previous => @previous, :original => @original, :argument_question => @argument_question, :argument_user => @argument_user}) %>';
if($('#<%= @previous+@node.id.to_s %>').length > 0){
  if('<%= @previous+@node.id.to_s %>' !== '<%= @previous+@original.to_s %>'){
    $('.icon-plus-sign.<%= @previous+@node.id.to_s %>').parent().hide();
    $('.icon-minus-sign.<%= @previous+@node.id.to_s %>').show();
    $('#<%= @previous+@node.id.to_s %>').replaceWith(j);
  }
}
else{
  $('#<%= @previous%>').html(j);
}
